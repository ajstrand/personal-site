{
  "plugins": [
    ["babel-plugin-transform-goober", { "name": "_styled" }],
    "babel-plugin-macros"
  ],
  "presets": [
    [
      "@babel/preset-react",
      {
        "pragma": "h", // default pragma is React.createElement (only in classic runtime)
        "pragmaFrag": "Fragment"
      }
    ]
  ]
}
