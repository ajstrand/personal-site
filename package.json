{
  "name": "my-personal-site",
  "version": "1.0.0",
  "main": "index.js",
  "author": "alex strand <ajstrand@gmail.com> (@_alex_strand)",
  "license": "MIT",
  "type": "module",
  "private": true,
  "workspaces": [],
  "scripts": {
    "devc": "vite",
    "dev": "node server.js",
    "b": "vite build",
    "site": "serve dist/static",
    "preview": "vite preview",
    "check": "bunx biome lint --write src",
    "lint": "bunx oxlint@latest",
    "test": "vitest",
    "build": "trash dist && bun run build:clientAssets && bun run build:server && node --experimental-specifier-resolution=node prerender",
    "build:clientAssets": "vite --config vite.config.client.js build --outDir dist/static",
    "build:server": "vite --config vite.config.server.js build --outDir dist/server --ssr src/entry-server.jsx"
  },
  "dependencies": {
    "@mdx-js/mdx": "3.1.0",
    "@mdx-js/preact": "3.1.0",
    "@mdx-js/rollup": "^3.1.0",
    "@preact/preset-vite": "^2.9.2",
    "@sindresorhus/slugify": "^2.2.1",
    "@stylexjs/dev-runtime": "^0.9.3",
    "@stylexjs/stylex": "^0.9.3",
    "@urql/preact": "^4.1.1",
    "express": "^4.21.1",
    "graphql": "^16.9.0",
    "graphql-request": "^7.1.2",
    "lucide-preact": "^0.464.0",
    "preact": "^10.25.0",
    "prop-types": "15.8.1",
    "vite": "^6.0.3",
    "vite-plugin-stylex": "^0.13.0"
  },
  "devDependencies": {
    "@shikijs/rehype": "^1.24.0",
    "shiki": "^1.24.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-parse": "^8.0.4",
    "rehype-slug": "^6.0.0",
    "fs-extra": "^11.2.0",
    "a11y-inspector": "file:../a11y-inspector/",
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@biomejs/biome": "1.9.4",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/preact": "^3.2.4",
    "@testing-library/user-event": "^14.5.2",
    "dayjs": "1.11.13",
    "eslint": "^9.16.0",
    "eslint-plugin-testing-library": "^6.4.0",
    "jsdom": "^25.0.1",
    "msw": "2.6.6",
    "oxlint": "^0.14.1",
    "path": "^0.12.7",
    "preact-render-to-string": "6.5.11",
    "rehype-katex": "^7.0.1",
    "remark-math": "^6.0.0",
    "rollup-plugin-esbuild": "^6.1.1",
    "serve": "^14.0.1",
    "vite-plugin-node-polyfills": "^0.22.0",
    "vitest": "2.1.8"
  }
