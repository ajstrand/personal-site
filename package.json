    "eslint": "^9.21.0",
    "eslint-plugin-testing-library": "^6.4.0",
    "fs-extra": "^11.3.0",
    "jsdom": "^25.0.1",
    "msw": "2.7.0",
    "oxlint": "0.16.2",
    "path": "^0.12.7",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-katex": "^7.0.1",
    "rehype-parse": "^9.0.1",
    "rehype-slug": "^6.0.0",
    "remark-math": "^6.0.0",
    "rollup-plugin-esbuild": "^6.1.1",
    "serve": "^14.2.4",
