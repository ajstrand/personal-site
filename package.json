{
  "name": "my-personal-site",
  "version": "1.0.0",
  "main": "index.js",
  "author": "alex strand <ajstrand@gmail.com> (@_alex_strand)",
  "license": "MIT",
  "type": "module",
  "private": true,
  "workspaces": [
    "workspace-a"
  ],
  "scripts": {
    "postinstall": "./postinstall.js",
    "dev": "yarn run build && yarn run serve",
    "serve": "serve public",
    "build": "toast incremental .",
    "breadbox": "breadbox --dest public/web_modules",
    "prepare": "husky install"
  },
  "dependencies": {
    "esinstall": "1.1.7",
    "@babel/runtime": "^7.15.4",
    "@emotion/core": "file:forkedPackages/emotion",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/preact": "^1.6.22",
    "@sector/breadbox": "^0.0.4",
    "@sindresorhus/slugify": "^2.1.0",
    "@toastdotdev/mdx": "0.2.4",
    "chalk": "^4.1.2",
    "facepaint": "^1.2.1",
    "fs-extra": "^10.0.0",
    "globby": "^11.0.4",
    "json5": "^2.2.0",
    "parse-numeric-range": "^1.3.0",
    "prism-react-renderer": "^1.2.1",
    "prismjs": "^1.24.1",
    "react": "^17.0.2",
    "react-feather": "^2.0.9",
    "react-gif-player": "^0.4.2",
    "react-intl": "^5.20.10",
    "rehype-autolink-headings": "^6.1.0",
    "rehype-parse": "^8.0.3",
    "rehype-slug": "^5.0.0",
    "toast": "^0.3.46",
    "unified": "^9.2.1"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "husky": "^7.0.0",
    "jest": "^27.0.6",
    "jest-cli": "^27.0.6",
    "prettier": "2.3.2",
    "pretty-quick": "^3.1.1",
    "react-axe": "^3.5.4",
    "serve": "^12.0.0",
    "workspace-a": "1.0.0",
    "postcss": "8.3.6",
    "autoprefixer": "^10.3.4",
    "pretty-bytes": "5.6.0"
  },
  "snowpack": {
    "exclude": [
      "fetch-mdx-post-files.js",
      "toast.js",
      "rehype-prism-mdx.js",
      "public",
      ".tmp"
    ],
    "installOptions": {
      "alias": {
        "react": "preact/compat",
        "preact/hooks": "preact/hooks/dist/hooks.module.js"
      }
    },
    "knownEntrypoints": [
      "prop-types",
      "preact",
      "preact/hooks",
      "@emotion/core",
      "@mdx-js/preact",
      "react-helmet",
      "preact/compat",
      "styled-resume"
    ]
  }
}
