    "babel-preset-solid": "^1.9.5",
    "consola": "^3.4.0",
    "critical": "^7.2.1",
    "csso": "^5.0.5",
    "express": "4.21.2",
    "graphql": "^16.10.0",
    "graphql-request": "^7.1.2",
    "prop-types": "15.8.1",
    "solid-devtools": "^0.33.0",
    "solid-js": "1.9.5",
    "solid-jsx": "^1.1.4",
    "vite": "6.2.2",
    "vite-plugin-solid": "^2.11.6"
  },
  "devDependencies": {
    "vite-plugin-inspect": "11.0.0",
    "@babel/core": "^7.26.9",
    "@biomejs/biome": "1.9.4",
