  "license": "MIT",
  "type": "module",
  "private": true,
  "workspaces": [],
  "scripts": {
    "dev": "node server.js",
    "site": "serve dist/static",
    "preview": "vite preview",
    "check": "bunx biome lint --write src",
    "lint": "bunx oxlint@latest",
    "test": "vitest",
    "prerender": "node --experimental-specifier-resolution=node prerender",
    "build": "bun run build:clientAssets && bun run build:server && bun run prerender",
    "build:clientAssets": "vite build --outDir dist/static",
    "build:server": "vite build --outDir dist/server --ssr src/entry-server.jsx"
  },
  "dependencies": {
    "@mdx-js/mdx": "3.1.0",
    "@mdx-js/rollup": "3.1.0",
    "@sindresorhus/slugify": "^2.2.1",
