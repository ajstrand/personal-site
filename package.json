{
  "name": "my-personal-site",
  "version": "1.0.0",
  "main": "index.js",
  "author": "alex strand <ajstrand@gmail.com> (@_alex_strand)",
  "license": "MIT",
  "type": "module",
  "private": true,
  "workspaces": [
    "workspace-a"
  ],
  "scripts": {
    "dev": "bun server.js",
    "b": "vite build",
    "site": "serve dist/",
    "lint": "bunx oxlint@latest",
    "prepare": "husky install",
    "res:build": "rescript",
    "res:clean": "rescript clean",
    "res:dev": "rescript -w",
    "build": "trash dist && bun run build:clientAssets && bun run build:server && node --experimental-specifier-resolution=node prerender",
    "build:clientAssets": "vite --config vite.config.client.js build --outDir dist/static",
    "build:server": "vite --config vite.config.server.js build --outDir dist/server --ssr src/entry-server.jsx"
  },
  "dependencies": {
    "@babel/cli": "^7.25.9",
    "@babel/core": "^7.26.0",
    "@babel/runtime": "^7.26.0",
    "@mdx-js/mdx": "3.1.0",
    "@mdx-js/preact": "3.1.0",
    "@mdx-js/rollup": "^3.1.0",
    "@preact/preset-vite": "^2.9.1",
    "@rescript/core": "1.6.0",
    "@rescript/react": "^0.13.0",
    "@sindresorhus/slugify": "^2.1.0",
    "@stefanprobst/rehype-shiki": "2.2.1",
    "@stylexjs/stylex": "^0.9.3",
    "@tanstack/react-query": "^5.56.2",
    "express": "^4.21.1",
    "fs-extra": "^10.1.0",
    "globby": "^13.1.2",
    "graphql": "^16.9.0",
    "graphql-request": "^7.1.0",
    "json5": "^2.2.1",
    "parse-numeric-range": "^1.3.0",
    "preact": "^10.24.3",
    "react": "^16.9.0",
    "react-error-boundary": "^4.0.13",
    "react-gif-player": "^0.4.2",
    "react-intl": "^6.0.5",
    "react-query-rewind": "^2.1.13",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-parse": "^8.0.4",
    "rehype-slug": "^6.0.0",
    "rescript": "^11.1.4",
    "vite": "^5.4.11",
    "vite-plugin-stylex": "^0.13.0",
    "vitest": "^2.1.4"
  },
  "devDependencies": {
    "@babel/preset-env": "7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@testing-library/preact": "^3.2.4",
    "@testing-library/user-event": "^14.4.3",
    "autoprefixer": "^10.4.20",
    "dayjs": "1.11.13",
    "eslint": "^8.23.0",
    "eslint-plugin-testing-library": "^5.6.0",
    "husky": "^7.0.4",
    "oxlint": "^0.11.0",
    "path": "^0.12.7",
    "postcss": "^8.4.47",
    "preact-render-to-string": "6.5.11",
    "prettier": "2.7.1",
    "pretty-quick": "^3.1.3",
    "react-axe": "^3.5.4",
    "rehype-katex": "^7.0.1",
    "remark-math": "^6.0.0",
    "rollup-plugin-esbuild": "^6.1.1",
    "serve": "^14.0.1",
    "tailwindcss": "^3.4.14",
    "vite-plugin-node-polyfills": "^0.22.0",
    "workspace-a": "1.0.0"
  }
