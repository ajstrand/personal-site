{
  "name": "my-personal-site",
  "version": "1.0.0",
  "main": "index.js",
  "author": "alex strand <ajstrand@gmail.com> (@_alex_strand)",
  "license": "MIT",
  "type": "module",
  "private": true,
  "workspaces": [],
  "scripts": {
    "dev": "node server.js",
    "b": "vite build",
    "site": "serve dist/static",
    "preview": "vite preview",
    "check": "bunx biome lint --write src",
    "lint": "bunx oxlint@latest",
    "test": "vitest",
    "prerender": "node --experimental-specifier-resolution=node prerender",
    "build": "trash dist && bun run build:clientAssets && bun run build:server && bun run prerender && node copyFiles.js",
    "build:clientAssets": "vite --config vite.config.client.js build --outDir dist/static",
    "build:server": "vite --config vite.config.server.js build --outDir dist/server --ssr src/entry-server.jsx"
  },
  "dependencies": {
    "@mdx-js/mdx": "3.1.0",
    "@mdx-js/preact": "3.1.0",
    "@mdx-js/rollup": "^3.1.0",
    "@preact/preset-vite": "^2.9.4",
    "@sindresorhus/slugify": "^2.2.1",
    "@urql/preact": "^4.1.1",
    "consola": "^3.3.3",
    "critical": "^7.2.1",
    "csso": "^5.0.5",
    "express": "^4.21.2",
    "graphql": "^16.9.0",
    "graphql-request": "^7.1.2",
    "lucide-preact": "^0.468.0",
    "preact": "^10.26.4",
    "prop-types": "15.8.1",
    "vite": "6.2.6"
  },
  "devDependencies": {
    "@shikijs/rehype": "3.2.1",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-parse": "^9.0.1",
    "rehype-slug": "^6.0.0",
    "fs-extra": "^11.2.0",
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@biomejs/biome": "1.9.4",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/preact": "^3.2.4",
    "@testing-library/user-event": "^14.6.1",
    "dayjs": "1.11.13",
    "eslint": "^9.16.0",
    "eslint-plugin-testing-library": "7.1.1",
    "jsdom": "26.0.0",
    "msw": "2.7.0",
    "oxlint": "^0.16.4",
    "path": "^0.12.7",
    "preact-render-to-string": "6.5.13",
    "rehype-katex": "^7.0.1",
    "remark-math": "^6.0.0",
    "rollup-plugin-esbuild": "^6.2.1",
    "serve": "^14.2.4",
    "vite-plugin-node-polyfills": "^0.23.0",
    "vitest": "3.1.1"
  }
